entity_summary_df['five_star_count'] = entity_summary_df['five_star_count'].apply(lambda x : str(x))
entity_summary_df['rating_count'] = entity_summary_df['rating_count'].apply(lambda x : str(x))
entity_summary_df['final_rating'] = entity_summary_df['final_rating'].apply(lambda x : str(x))
entity_summary_df['krowd_rating'] = entity_summary_df['krowd_rating'].apply(lambda x : str(x))
entity_summary_df['krowd_score'] = entity_summary_df['krowd_score'].apply(lambda x : str(x))
entity_summary_df['positive_reviews_count'] = entity_summary_df['positive_reviews_count'].apply(lambda x : str(x))
entity_summary_df['negative_reviews_count'] = entity_summary_df['negative_reviews_count'].apply(lambda x : str(x))